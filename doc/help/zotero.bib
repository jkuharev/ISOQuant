@article{de_la_mora_current_1994,
	title = {The current emitted by highly conducting Taylor cones},
	volume = {260},
	issn = {1469-7645},
	url = {http://journals.cambridge.org/article_S0022112094003472},
	doi = {10.1017/S0022112094003472},
	abstract = {When a liquid meniscus held at the exit of a metallic capillary tube is charged to a high voltage V, the free surface often takes the form of a cone whose apex emits a steady microjet, and thus injects a certain charge I and liquid volume Q per unit time into the surrounding gas. This work deals with liquids with relatively large conductivities K, for which the jet diameter dj is much smaller than the diameter dn of the capillary tube. In the limit dj\&sol;dn {\textrightarrow} 0, the structure of the jet (dj and I, in particular) becomes independent of electrostatic parameters such as V or the electrode configuration, being governed mostly by the liquid properties and flow rate Q. Furthermore, the measured current is given approximately by I = f(\&epsi;) ($\gamma${QK}\&sol;\&epsi;)\&half; for a wide variety of liquids and conditions (\&epsi;, and $\gamma$ are, respectively, the dielectric constant of the liquid and the coefficient of interfacial tension; f(\&epsi;) is shown in figure 11). The following explanation is proposed for this behaviour. Convection associated with the liquid flow Q transports the net surface charge towards the cone tip. This upsets the electrostatic surface charge distribution slightly at distances r from the apex large compared to a certain charge relaxation length $\lambda$, but substantially when r \~{} $\lambda$. When the fluid motion is modelled as a sink flow, $\lambda$ is of the order of r* = (Q\&epsi;\&epsi;0\&sol;K)\${\textbackslash}frac13\$ (\&epsi;0 is the electrical permittivity of vacuum). If, in addition, the surface charge density is described through Taylor\&apos;s theory, the corresponding surface current convected towards the apex scales as Is \~{} ($\gamma${QK}\&sol;\&epsi;)\&half;, as observed for the spray current. The sink flow hypothesis is shown to be realistic for sufficiently small jet Reynolds numbers. In a few photographs of ethylene glycol cone jets, we find the rough scaling dj \~{} 0.4r* for the jet diameter, which shows that the jet forms as soon as charge relaxation effects set in. In the limit \&epsi; \&Gt; 1, an upper bound is found for the convected current at the virtual cone apex, which accounts for only one-quarter of the total measured spray current. The rest of the charge must accordingly reach the head of the jet by conduction through the bulk.},
	urldate = {2014-09-30},
	journal = {Journal of Fluid Mechanics},
	author = {De La Mora, J. Fern\&aacute;ndez and Loscertales, I. G.},
	month = feb,
	year = {1994},
	pages = {155--184},
	file = {Cambridge Journals Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/T3DKFNA7/displayAbstract.html:text/html}
}

