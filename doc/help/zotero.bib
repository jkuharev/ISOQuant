@article{lu_absolute_2007,
	title = {Absolute protein expression profiling estimates the relative contributions of transcriptional and translational regulation},
	volume = {25},
	copyright = {{\textcopyright} 2006 Nature Publishing Group},
	issn = {1087-0156},
	url = {http://www.nature.com/nbt/journal/v25/n1/abs/nbt1270.html},
	doi = {10.1038/nbt1270},
	abstract = {We report a method for large-scale absolute protein expression measurements ({APEX}) and apply it to estimate the relative contributions of transcriptional- and translational-level gene regulation in the yeast and Escherichia coli proteomes. {APEX} relies upon correcting each protein's mass spectrometry sampling depth (observed peptide count) by learned probabilities for identifying the peptides. {APEX} abundances agree with measurements from controls, western blotting, flow cytometry and two-dimensional gels, as well as known correlations with {mRNA} abundances and codon bias, providing absolute protein concentrations across approximately three to four orders of magnitude. Using {APEX}, we demonstrate that 73\% of the variance in yeast protein abundance (47\% in E. coli) is explained by {mRNA} abundance, with the number of proteins per {mRNA} log-normally distributed about {\textasciitilde}5,600 ({\textasciitilde}540 in E. coli) protein molecules/{mRNA}. Therefore, levels of both eukaryotic and prokaryotic proteins are set per {mRNA} molecule and independently of overall protein concentration, with {\textgreater}70\% of yeast gene expression regulation occurring through {mRNA}-directed mechanisms.},
	language = {en},
	number = {1},
	urldate = {2014-10-02},
	journal = {Nature Biotechnology},
	author = {Lu, Peng and Vogel, Christine and Wang, Rong and Yao, Xin and Marcotte, Edward M.},
	month = jan,
	year = {2007},
	pages = {117--124},
	file = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/FKVVK2FE/Lu et al. - 2007 - Absolute protein expression profiling estimates th.pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/U2EI55UH/nbt1270.html:text/html}
