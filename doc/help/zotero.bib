	keywords = {Differential expression, Quantitative analysis, Shotgun proteomics, Signal transduction profiling},
	pages = {994--999},
	file = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/SERX8GNS/Asara et al. - 2008 - A label-free quantification method by MSMS TIC co.pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/BV73UB3A/abstract\;jsessionid=4BB87ED6E89D83984F0A5BB6DE0BADFB.html:text/html}
