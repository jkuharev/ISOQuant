@article{kim_spectral_2008,
	title = {Spectral Probabilities and Generating Functions of Tandem Mass Spectra: A Strike against Decoy Databases},
	volume = {7},
	issn = {1535-3893},
	shorttitle = {Spectral Probabilities and Generating Functions of Tandem Mass Spectra},
	url = {http://dx.doi.org/10.1021/pr8001244},
	doi = {10.1021/pr8001244},
	abstract = {A key problem in computational proteomics is distinguishing between correct and false peptide identifications. We argue that evaluating the error rates of peptide identifications is not unlike computing generating functions in combinatorics. We show that the generating functions and their derivatives (spectral energy and spectral probability) represent new features of tandem mass spectra that, similarly to ?-scores, significantly improve peptide identifications. Furthermore, the spectral probability provides a rigorous solution to the problem of computing statistical significance of spectral identifications. The spectral energy/probability approach improves the sensitivity-specificity tradeoff of existing {MS}/{MS} search tools, addresses the notoriously difficult problem of ?one-hit-wonders? in mass spectrometry, and often eliminates the need for decoy database searches. We therefore argue that the generating function approach has the potential to increase the number of peptide identifications in {MS}/{MS} searches.},
	number = {8},
	urldate = {2014-09-25},
	journal = {Journal of Proteome Research},
	author = {Kim, Sangtae and Gupta, Nitin and Pevzner, Pavel A.},
	month = aug,
	year = {2008},
	pages = {3354--3363},
	file = {ACS Full Text PDF w/ Links:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/3RSCAEHG/Kim et al. - 2008 - Spectral Probabilities and Generating Functions of.pdf:application/pdf;ACS Full Text Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/6EF55VZ9/pr8001244.html:text/html}
}

