	author = {Eng, Jimmy K. and McCormack, Ashley L. and Yates, John R.},
	month = nov,
	year = {1994},
	keywords = {Analytical Chemistry, Bioinformatics, Biotechnology, Organic Chemistry, Proteomics},
	pages = {976--989},
	file = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/BXPTSUMK/Eng et al. - 1994 - An approach to correlate tandem mass spectral data.pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/J6ERZI9M/10.html:text/html}
}

