	author = {Gygi, Steven P. and Rochon, Yvan and Franza, B. Robert and Aebersold, Ruedi},
	month = jan,
	year = {1999},
	pmid = {10022859},
	pages = {1720--1730},
	file = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/XNDP8S8P/Gygi et al. - 1999 - Correlation between Protein and mRNA Abundance in .pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/FE2T6I8P/1720.html:text/html}
}

@article{mcdaniel_drift_1962,
	title = {Drift Tube-Mass Spectrometer for Studies of Low-Energy Ion-Molecule Reactions},
