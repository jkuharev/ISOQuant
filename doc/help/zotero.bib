	journal = {Proceedings of the National Academy of Sciences},
	author = {Taverna, Sean D. and Ueberheide, Beatrix M. and Liu, Yifan and Tackett, Alan J. and Diaz, Robert L. and Shabanowitz, Jeffrey and Chait, Brian T. and Hunt, Donald F. and Allis, C. David},
	month = feb,
	year = {2007},
	pmid = {17284592},
	keywords = {bivalent domain, electron transfer dissociation, Mass Spectrometry, posttranslational modifications, Tetrahymena},
	pages = {2086--2091},
	file = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/R7W5SP6I/Taverna et al. - 2007 - Long-distance combinatorial linkage between methyl.pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/7PMM84IK/2086.html:text/html}
