	author = {Perkins, David N. and Pappin, Darryl J. C. and Creasy, David M. and Cottrell, John S.},
	month = dec,
	year = {1999},
	keywords = {Bioinformatics, Mass Spectrometry, Protein identification},
	pages = {3551--3567},
	file = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/NGQK6KK9/Perkins et al. - 1999 - Probability-based protein identification by search.pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/UR4674MM/abstract.html:text/html}
}

@article{washburn_large-scale_2001,
