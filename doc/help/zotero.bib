	author = {Reisinger, Florian and Krishna, Ritesh and Ghali, Fawaz and R{\'i}os, Daniel and Hermjakob, Henning and Antonio Vizca{\'i}no, Juan and Jones, Andrew R.},
	month = mar,
	year = {2012},
	keywords = {Bioinformatics, Java {API}, {mzIdentML}, Proteomics standards initiative ({PSI}), {XML}},
	pages = {790--794},
	file = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/MCGBR3JF/Reisinger et al. - 2012 - jmzIdentML API A Java interface to the mzIdentML .pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/ZVT9M8NH/full.html:text/html}
