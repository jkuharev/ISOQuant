	pages = {5--8},
	file = {ScienceDirect Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/JQMGK8BN/Yates III - 2000 - Mass spectrometry from genomics to proteomics.pdf:application/pdf;ScienceDirect Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/VAGW9K94/S016895259901879X.html:text/html}
}

@article{horn_automated_2000,
	title = {Automated de novo sequencing of proteins by tandem high-resolution mass spectrometry},
	volume = {97},
	issn = {0027-8424, 1091-6490},
	url = {http://www.pnas.org/content/97/19/10313},
	doi = {10.1073/pnas.97.19.10313},
	abstract = {A de novo sequencing program for proteins is described that uses tandem {MS} data from electron capture dissociation and collisionally activated dissociation of electrosprayed protein ions. Computer automation is used to convert the fragment ion mass values derived from these spectra into the most probable protein sequence, without distinguishing Leu/Ile. Minimum human input is necessary for the data reduction and interpretation. No extra chemistry is necessary to distinguish N- and C-terminal fragments in the mass spectra, as this is determined from the electron capture dissociation data. With parts-per-million mass accuracy (now available by using higher field Fourier transform {MS} instruments), the complete sequences of ubiquitin (8.6 {kDa}) and melittin (2.8 {kDa}) were predicted correctly by the program. The data available also provided 91\% of the cytochrome c (12.4 {kDa}) sequence (essentially complete except for the tandem {MS}-resistant region K13{\textendash}V20 that contains the cyclic heme). Uncorrected mass values from a 6-T instrument still gave 86\% of the sequence for ubiquitin, except for distinguishing Gln/Lys. Extensive sequencing of larger proteins should be possible by applying the algorithm to pieces of ?10-{kDa} size, such as products of limited proteolysis.},
	language = {en},
	number = {19},
	urldate = {2014-05-15},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Horn, David M. and Zubarev, Roman A. and McLafferty, Fred W.},
	month = dec,
	year = {2000},
	pmid = {10984529},
	keywords = {electron capture dissociation, electrospray ionization, Fourier transform {MS}},
	pages = {10313--10317},
	file = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/ZVR6RSJF/Horn et al. - 2000 - Automated de novo sequencing of proteins by tandem.pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/VTVT2FNI/10313.html:text/html}
}

@article{serang_review_2012,
	title = {A review of statistical methods for protein identification using tandem mass spectrometry},
	volume = {5},
	issn = {1938-7989},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3402235/},
	abstract = {Tandem mass spectrometry has emerged as a powerful tool for the characterization of complex protein samples, an increasingly important problem in biology. The effort to efficiently and accurately perform inference on data from tandem mass spectrometry experiments has resulted in several statistical methods. We use a common framework to describe the predominant methods and discuss them in detail. These methods are classified using the following categories: set cover methods, iterative methods, and Bayesian methods. For each method, we analyze and evaluate the outcome and methodology of published comparisons to other methods; we use this comparison to comment on the qualities and weaknesses, as well as the overall utility, of all methods. We discuss the similarities between these methods and suggest directions for the field that would help unify these similar assumptions in a more rigorous manner and help enable efficient and reliable protein inference.},
	number = {1},
	urldate = {2014-06-16},
	journal = {Statistics and its interface},
	author = {Serang, Oliver and Noble, William},
	year = {2012},
	pmid = {22833779},
	pmcid = {PMC3402235},
	pages = {3--20},
	file = {PubMed Central Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/7E4QAIX7/Serang und Noble - 2012 - A review of statistical methods for protein identi.pdf:application/pdf}
}

@article{mcdonald_shotgun_2002,
	title = {Shotgun Proteomics and Biomarker Discovery},
	volume = {18},
	issn = {0278-0240},
	url = {http://www.hindawi.com/journals/dm/2002/505397/abs/},
	doi = {10.1155/2002/505397},
	abstract = {Coupling large-scale sequencing projects with the amino acid sequence information that can be gleaned from tandem mass spectrometry ({MS}/{MS}) has made it much easier to analyze complex mixtures of proteins. The limits of this \&\#x201c;shotgun\&\#x201d; approach, in which the protein mixture is proteolytically digested before separation, can be further expanded by separating the resulting mixture of peptides prior to {MS}/{MS} analysis. Both single dimensional high pressure liquid chromatography ({LC}) and multidimensional {LC} ({LC}/{LC}) can be directly interfaced with the mass spectrometer to allow for automated collection of tremendous quantities of data. While there is no single technique that addresses all proteomic challenges, the shotgun approaches, especially {LC}/{LC}-{MS}/{MS}-based techniques such as {MudPIT} (multidimensional protein identification technology), show advantages over gel-based techniques in speed, sensitivity, scope of analysis, and dynamic range. Advances in the ability to quantitate differences between samples and to detect for an array of post-translational modifications allow for the discovery of classes of protein biomarkers that were previously unassailable.},
	language = {en},
	number = {2},
	urldate = {2014-06-04},
	journal = {Disease Markers},
	author = {McDonald, W. Hayes and Yates, John R.},
	year = {2002},
	pages = {99--105},
	file = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/2TEJH59V/McDonald und Yates - 2002 - Shotgun Proteomics and Biomarker Discovery.pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/VUPAPGT5/abs.html:application/xhtml+xml}
}

@article{forner_quantitative_2006,
	title = {Quantitative Proteomic Comparison of Rat Mitochondria from Muscle, Heart, and Liver},
	volume = {5},
	issn = {1535-9476, 1535-9484},
	url = {http://www.mcponline.org/content/5/4/608},
	doi = {10.1074/mcp.M500298-MCP200},
	abstract = {Mitochondria, through oxidative phosphorylation, are the primary source of energy production in all tissues under aerobic conditions. Although critical to life, energy production is not the only function of mitochondria, and the composition of this organelle is tailored to meet the specific needs of each cell type. As an organelle, the mitochondrion has been a popular subject for proteomic analysis, but quantitative proteomic methods have yet to be applied to tease apart subtle differences among mitochondria from different tissues or muscle types. Here we used mass spectrometry-based proteomics to analyze mitochondrial proteins extracted from rat skeletal muscle, heart, and liver tissues. Based on 689 proteins identified with high confidence, mitochondria from the different tissues are qualitatively quite similar. However, striking differences emerged from the quantitative comparison of protein abundance between the tissues. Furthermore we applied similar methods to analyze mitochondrial matrix and intermembrane space proteins extracted from the same mitochondrial source, providing evidence for the submitochondrial localization of a number of proteins in skeletal muscle and liver. Several proteins not previously thought to reside in mitochondria were identified, and their presence in this organelle was confirmed by protein correlation profiling. Hierarchical clustering of microarray expression data provided further evidence that some of the novel mitochondrial candidates identified in the proteomic survey might be associated with mitochondria. These data reveal several important distinctions between mitochondrial and submitochondrial proteomes from skeletal muscle, heart, and liver tissue sources. Indeed approximately one-third of the proteins identified in the soluble fractions are associated predominantly to one of the three tissues, indicating a tissue-dependent regulation of mitochondrial proteins. Furthermore a small percentage of the mitochondrial proteome is unique to each tissue.},
	language = {en},
	number = {4},
	urldate = {2014-07-03},
	journal = {Molecular \& Cellular Proteomics},
	author = {Forner, Francesca and Foster, Leonard J. and Campanaro, Stefano and Valle, Giorgio and Mann, Matthias},
	month = jan,
	year = {2006},
	pmid = {16415296},
	pages = {608--619},
	file = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/IV7MQGHN/Forner et al. - 2006 - Quantitative Proteomic Comparison of Rat Mitochond.pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/A5HMINQF/608.html:text/html}
}

@article{ballardini_massuntangler_2011,
	title = {{MassUntangler}: A novel alignment tool for label-free liquid chromatography{\textendash}mass spectrometry proteomic data},
	volume = {1218},
	issn = {0021-9673},
	shorttitle = {Theoretical and Practical Aspects in the Separation of Large Biomolecules},
	url = {http://www.sciencedirect.com/science/article/pii/S0021967311008776},
	doi = {10.1016/j.chroma.2011.06.062},
	abstract = {Liquid chromatography{\textendash}mass spectrometry ({LC}{\textendash}{MS}) has become an important analytical tool for quantitative proteomics and biomarker discovery. In the label-free differential {LC}{\textendash}{MS} approach computational methods are required for an accurate alignment of peaks extrapolated from the experimental raw data accounting for retention time and m/z signals intensity, which are strongly affected by sample matrix and instrumental performance. A novel procedure {\textquotedblleft}{MassUntangler}{\textquotedblright} for pairwise alignment has been developed, relying on a pattern-based matching algorithm integrated with filtering algorithms in a multi-step approach. The procedure has been optimized employing a two-step approach. Firstly, low-complexity {LC}{\textendash}{MS} data derived from the enzymatic digestion of two standard proteins have been analyzed. Then, the algorithm's performance has been evaluated by comparing the results with other achieved using state-of-the-art alignment tools. In the second step, our algorithm has been used for the alignment of high-complexity {LC}{\textendash}{MS} data consisting of peptides obtained by an Escherichia coli lysate available from a public repository previously used for the comparison of other alignment tools. {MassUntangler} gave excellent results in terms of precision scores (from 80\% to 93\%) and recall scores (from 68\% to 89\%), showing performances similar and even better than the previous developed tools. Considering the mass spectrometry sensitivity and accuracy, this approach allows the identification and quantification of peptides present in a biological sample at femtomole level with high confidence. The procedure's capability of aligning {LC}{\textendash}{MS} data previously corrected for distortion in retention time has been studied through a hybrid approach, in which {MassUntangler} was interfaced with the {OpenMS} {TOPP} tool {MapAligner}. The hybrid aligner yielded better results, showing that an integration of different bioinformatic approaches for accurate label-free {LC}{\textendash}{MS} data alignment should be used.},
	number = {49},
	urldate = {2012-11-22},
	journal = {Journal of Chromatography A},
	author = {Ballardini, R. and Benevento, M. and Arrigoni, G. and Pattini, L. and Roda, A.},
	month = dec,
	year = {2011},
	keywords = {Bioinformatics, Differential {LC}{\textendash}{MS} proteomics, E. coli, Liquid chromatography{\textendash}mass spectrometry},
	pages = {8859--8868},
	file = {ScienceDirect Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/A64PTUPG/Ballardini et al. - 2011 - MassUntangler A novel alignment tool for label-fr.pdf:application/pdf}
}

@article{michel_mast_2013,
	title = {Mast Cell{\textendash}deficient {KitW}-sh {\textquotedblleft}Sash{\textquotedblright} Mutant Mice Display Aberrant Myelopoiesis Leading to the Accumulation of Splenocytes That Act as Myeloid-Derived Suppressor Cells},
	volume = {190},
	issn = {0022-1767, 1550-6606},
	url = {http://www.jimmunol.org/content/190/11/5534},
	doi = {10.4049/jimmunol.1203355},
	abstract = {Mast cell-deficient {KitW}-sh {\textquotedblleft}sash{\textquotedblright} mice are widely used to investigate mast cell functions. However, mutations of c-Kit also affect additional cells of hematopoietic and nonimmune origin. In this study, we demonstrate that {KitW}-sh causes aberrant extramedullary myelopoiesis characterized by the expansion of immature lineage-negative cells, common myeloid progenitors, and granulocyte/macrophage progenitors in the spleen. A consistent feature shared by these cell types is the reduced expression of c-Kit. Populations expressing intermediate and high levels of Ly6G, a component of the myeloid differentiation Ag Gr-1, are also highly expanded in the spleen of sash mice. These cells are able to suppress T cell responses in vitro and phenotypically and functionally resemble myeloid-derived suppressor cells ({MDSC}). {MDSC} typically accumulate in tumor-bearing hosts and are able to dampen immune responses. Consequently, transfer of {MDSC} from naive sash mice into line 1 alveolar cell carcinoma tumor-bearing wild-type littermates leads to enhanced tumor progression. However, although it can also be observed in sash mice, accelerated growth of transplanted line 1 alveolar cell carcinoma tumors is a mast cell{\textendash}independent phenomenon. Thus, the {KitW}-sh mutation broadly affects key steps in myelopoiesis that may have an impact on mast cell research.},
	language = {en},
	number = {11},
	urldate = {2015-02-10},
	journal = {The Journal of Immunology},
	author = {Michel, Anastasija and Sch{\"u}ler, Andrea and Friedrich, Pamela and D{\"o}ner, Fatma and Bopp, Tobias and Radsak, Markus and Hoffmann, Markus and Relle, Manfred and Distler, Ute and Kuharev, J{\"o}rg and Tenzer, Stefan and Feyerabend, Thorsten B. and Rodewald, Hans-Reimer and Schild, Hansj{\"o}rg and Schmitt, Edgar and Becker, Marc and Stassen, Michael},
	month = jan,
	year = {2013},
	pmid = {23636054},
	pages = {5534--5544},
	file = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/HIMGGXZB/Michel et al. - 2013 - Mast Cell{\textendash}deficient KitW-sh {\textquotedblleft}Sash{\textquotedblright} Mutant Mice Dis.pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/HRCP4M56/5534.html:text/html}
}

@article{van_nederkassel_chemometric_2006,
	series = {29th International Symposium on High Performance Liquid Phase Separations and Related Techniques Part {II}},
