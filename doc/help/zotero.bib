@article{vizcaino_proteomics_2013,
	title = {The Proteomics Identifications ({PRIDE}) database and associated tools: status in 2013},
	volume = {41},
	issn = {0305-1048, 1362-4962},
	shorttitle = {The Proteomics Identifications ({PRIDE}) database and associated tools},
