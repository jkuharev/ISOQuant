@article{shevchenko_linking_1996,
	title = {Linking genome and proteome by mass spectrometry: Large-scale  identification of yeast proteins from two dimensional gels},
	volume = {93},
	issn = {0027-8424},
	shorttitle = {Linking genome and proteome by mass spectrometry},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC26151/},
	abstract = {The function of many of the uncharacterized open reading frames
 discovered by genomic sequencing can be determined at the level of
 expressed gene products, the proteome. However, identifying the cognate
 gene from minute amounts of protein has been one of the major problems
 in molecular biology. Using yeast as an example, we demonstrate here
 that mass spectrometric protein identification is a general solution to
 this problem given a completely sequenced genome. As a first screen,
 our strategy uses automated laser desorption ionization mass
 spectrometry of the peptide mixtures produced by in-gel tryptic
 digestion of a protein. Up to 90\% of proteins are identified by
 searching sequence data bases by lists of peptide masses obtained with
 high accuracy. The remaining proteins are identified by partially
 sequencing several peptides of the unseparated mixture by
 nanoelectrospray tandem mass spectrometry followed by data base
 searching with multiple peptide sequence tags. In blind trials,
 the method led to unambiguous identification in all cases. In the
 largest individual protein identification project to date, a total of
 150 gel spots{\textemdash}many of them at subpicomole amounts{\textemdash}were successfully
 analyzed, greatly enlarging a yeast two-dimensional gel data base. More
 than 32 proteins were novel and matched to previously uncharacterized
 open reading frames in the yeast genome. This study establishes that
 mass spectrometry provides the required throughput, the certainty of
 identification, and the general applicability to serve as the method of
 choice to connect genome and proteome.},
	number = {25},
	urldate = {2014-05-12},
	journal = {Proceedings of the National Academy of Sciences of the United States of America},
	author = {Shevchenko, Andrej and Jensen, Ole N. and Podtelejnikov, Alexandre V. and Sagliocco, Francis and Wilm, Matthias and Vorm, Ole and Mortensen, Peter and Shevchenko, Anna and Boucherie, Helian and Mann, Matthias},
	month = dec,
	year = {1996},
	pmid = {8962070},
	pmcid = {PMC26151},
	pages = {14440--14445},
	file = {PubMed Central Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/8NVNXISM/Shevchenko et al. - 1996 - Linking genome and proteome by mass spectrometry .pdf:application/pdf}
}

@article{gygi_evaluation_2000,
	title = {Evaluation of Two-Dimensional Gel Electrophoresis Based Proteome Analysis Technology},
	volume = {97},
	copyright = {Copyright {\textcopyright} 2000 National Academy of Sciences},
	issn = {0027-8424},
	url = {http://www.jstor.org/stable/123473},
	abstract = {Proteome analysis is most commonly accomplished by a combination of two-dimensional gel electrophoresis (2DE) to separate and visualize proteins and mass spectrometry ({MS}) for protein identification. Although this technique is powerful, mature, and sensitive, questions remain concerning its ability to characterize all of the elements of a proteome. In the current study, more than 1,500 features were visualized by silver staining a narrow {pH} range (4.9-5.7) 2D gel in which 0.5 mg of total soluble yeast protein was separated. Fifty spots migrating to a region of 4 cm2 were subjected to {MS} protein identification. Despite the high sample load and extended electrophoretic separation, proteins from genes with codon bias values of {\textless}0.1 (lower abundance proteins) were not found, even though fully one-half of all yeast genes fall into that range. Proteins from genes with codon bias values of {\textless}0.1 were found, however, if protein amounts exceeding the capacity of 2DE were fractionated and analyzed. We conclude that the large range of protein expression levels limits the ability of the 2DE-{MS} approach to analyze proteins of medium to low abundance, and thus the potential of this technique for proteome analysis is likewise limited.},
	number = {17},
	urldate = {2015-02-06},
	journal = {Proceedings of the National Academy of Sciences of the United States of America},
	author = {Gygi, Steven P. and Corthals, Garry L. and Zhang, Yanni and Rochon, Yvan and Aebersold, Ruedi},
	month = aug,
	year = {2000},
	pages = {9390--9395},
	file = {JSTOR Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/JQJUGD4F/Gygi et al. - 2000 - Evaluation of Two-Dimensional Gel Electrophoresis .pdf:application/pdf}
}

@article{prieto_panalyzer_2012,
	title = {{PAnalyzer}: A software tool for protein inference in shotgun proteomics},
