	month = jan,
	year = {1995},
	keywords = {Amino acid analysis, Cross-species mapping, Mycoplasma genitalium, Rapid protein identification, Tubulin, Two-dimensional polyacrylamide gel electrophoresis, Mass spectrometry},
	pages = {1090--1094},
	file = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/GI5TJTTI/Wasinger et al. - 1995 - Progress with gene-product mapping of the Mollicut.pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/RKFJ7JCE/full.html:text/html}
