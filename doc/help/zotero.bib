@article{yates_iii_mass_2000,
	title = {Mass spectrometry: from genomics to proteomics},
	volume = {16},
	issn = {0168-9525},
	shorttitle = {Mass spectrometry},
	url = {http://www.sciencedirect.com/science/article/pii/S016895259901879X},
	doi = {10.1016/S0168-9525(99)01879-X},
	abstract = {Large-scale {DNA} sequencing has stimulated the development of proteomics by providing a sequence infrastructure for protein analysis. Rapid and automated protein identification can be achieved by searching protein and nucleotide sequence databases directly with data generated by mass spectrometry. A high-throughput and large-scale approach to identifying proteins has been the result. These technological changes have advanced protein expression studies and the identification of proteins in complexes, two types of studies that are essential in deciphering the networks of proteins that are involved in biological processes.},
	number = {1},
	urldate = {2014-09-16},
	journal = {Trends in Genetics},
	author = {Yates III, John R.},
	month = jan,
	year = {2000},
