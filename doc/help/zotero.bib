
@article{comisarow_fourier_1974,
	title = {Fourier transform ion cyclotron resonance spectroscopy},
	volume = {25},
	issn = {0009-2614},
	url = {http://www.sciencedirect.com/science/article/pii/0009261474891372},
	doi = {10.1016/0009-2614(74)89137-2},
	abstract = {An ion cyclotron resonance ({ICR}) absorption spectrum has been obtained by exciting an {ICR} spectral segment with a fixed-frequency electric field pulse, followed by broad-band detection, digitization of the (time-domain) transient response, and digital Fourier transformation to produce the (frequency-domain) absorption spectrum. For a given signal-to-noise ratio and resolution, the {FT}-{ICR} method generates a spectrum in a time which is two orders of magnitude shorter than that required in conventional slow-sweep {ICR} detection. In the present example, a signal-to-noise ratio of 8:1 and a mass resolution of about 0.005 amu for {CH}4+ (from {CH}4 at a pressure of 8 X 10-7 torr) have been achieved, using a single data acquisition period of 25.6 msec.},
	number = {2},
	urldate = {2014-09-11},
	journal = {Chemical Physics Letters},
	author = {Comisarow, Melvin B. and Marshall, Alan G.},
	month = mar,
	year = {1974},
	pages = {282--283},
	file = {ScienceDirect Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/KFVTAZ73/Comisarow und Marshall - 1974 - Fourier transform ion cyclotron resonance spectros.pdf:application/pdf;ScienceDirect Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/57HIQF7E/0009261474891372.html:text/html}
}

@article{pringle_investigation_2007,
	title = {An investigation of the mobility separation of some peptide and protein ions using a new hybrid quadrupole/travelling wave {IMS}/oa-{ToF} instrument},
	volume = {261},
	issn = {1387-3806},
	url = {http://www.sciencedirect.com/science/article/pii/S138738060600354X},
	doi = {10.1016/j.ijms.2006.07.021},
	abstract = {Ion mobility coupled with mass spectrometry has evolved into a powerful analytical technique for investigating the gas-phase structures of bio-molecules. Here we present the mobility separation of some peptide and protein ions using a new hybrid quadrupole/travelling wave ion mobility separator/orthogonal acceleration time-of-flight instrument. Comparison of the mobility data obtained from the relatively new travelling wave separation device with data obtained using various other mobility separators demonstrate that whilst the mobility characteristics are similar, the new hybrid instrument geometry provides mobility separation without compromising the base sensitivity of the mass spectrometer. This capability facilitates mobility studies of samples at analytically significant levels.},
	number = {1},
	urldate = {2014-09-12},
	journal = {International Journal of Mass Spectrometry},
	author = {Pringle, Steven D. and Giles, Kevin and Wildgoose, Jason L. and Williams, Jonathan P. and Slade, Susan E. and Thalassinos, Konstantinos and Bateman, Robert H. and Bowers, Michael T. and Scrivens, James H.},
	month = mar,
	year = {2007},
	keywords = {Conformation, Ion mobility, Mass Spectrometry, Travelling wave},
	pages = {1--12},
	file = {ScienceDirect Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/4R53QMXQ/Pringle et al. - 2007 - An investigation of the mobility separation of som.pdf:application/pdf;ScienceDirect Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/7xq211hv.default-1411579827487/zotero/storage/H2MHGWDP/S138738060600354X.html:text/html}
}

